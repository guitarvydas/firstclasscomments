Main [@facts] = [[${facts}]]
Fact [f] = [[${f}\n]]
ValueFact [kvalue lpar id kcomma str rpardot] = [[${kvalue}${lpar}${id}${kcomma}${support.mangleNewlines (str)}${rpardot}]]
OtherFact [rel lpar id kcomma obj rpardot] = [[${rel}${lpar}${id}${kcomma}${obj}${rpardot}]]
Object [o] = [[${o}]]
ident [@cs] = [[${cs}]]
number [@ds] = [[${ds}]]
string [q1 @cs q2] = [[${q1}${cs}${q2}]]
quote [c] = [[${c}]]
notQuote [c] = [[${c}]]
